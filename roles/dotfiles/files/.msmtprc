defaults
logfile ~/.mutt/msmtp.log
host smtp.office365.com
port 587
protocol smtp
tls on
auth on
tls_starttls on
tls_trust_file /usr/local/etc/openssl/cert.pem

account reecon
from br@reecon.at
user br@reecon.at
passwordeval "security find-generic-password -s mbsync-reecon-password -w"

account robotic
from bernhard.reitinger@robotic-eyes.com
user bernhard.reitinger@robotic-eyes.com
passwordeval "security find-generic-password -s mbsync-robotic-password -w"

account live
host smtp-mail.outlook.com
from bernhard.reitinger@live.com
user bernhard.reitinger@live.com
passwordeval "security find-generic-password -s mbsync-live-password -w"

account default : reecon
